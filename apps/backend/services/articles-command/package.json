{
   "name": "backend-services-articles-command",
   "version": "1.0.0",
   "main": "src/main.js",
   "type": "module",
   "scripts": {
      "dev": "NODE_ENV='development' nodemon -e ts --exec 'pnpm build && node dist/main.js' --signal SIGTERM",
      "start": "NODE_ENV='production' node dist/main.js",
      "build": "esbuild src/main.ts --outfile=dist/main.js --platform=node --target=node16.13.0 --format=esm",
      "format": "prettier --write ."
   },
   "devDependencies": {
      "@nestjs/common": "8.2.4",
      "@types/express": "4.17.13",
      "esbuild": "0.14.6",
      "nodemon": "2.0.15",
      "prettier": "2.5.1",
      "typescript": "4.5.4"
   },
   "dependencies": {
      "express": "4.17.2"
   }
}
