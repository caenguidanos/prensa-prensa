{
   "name": "@workspace/domain-articles",
   "version": "1.0.0",
   "main": "dist/cjs/main.js",
   "module": "dist/esm/main.js",
   "types": "dist/types/main.d.ts",
   "exports": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
   },
   "scripts": {
      "lint": "eslint .",
      "build": "rimraf dist && tsc && pnpm build:cjs && pnpm build:esm",
      "build:cjs": "esbuild ./src/main.ts --bundle --platform=neutral --sourcemap --target=es2015 --outdir=dist/cjs --format=cjs --minify",
      "build:esm": "esbuild ./src/main.ts --bundle --platform=neutral --sourcemap --target=esnext --outdir=dist/esm --format=esm --minify",
      "test": "jest --no-cache",
      "format": "prettier --write ."
   },
   "devDependencies": {
      "@types/node": "17.0.2",
      "@typescript-eslint/eslint-plugin": "5.7.0",
      "@typescript-eslint/parser": "5.7.0",
      "esbuild": "0.14.7",
      "eslint-config-prettier": "8.3.0",
      "eslint": "8.5.0",
      "jest": "27.4.5",
      "prettier": "2.5.1",
      "rimraf": "3.0.2",
      "ts-jest": "27.1.2",
      "typescript": "4.5.4"
   },
   "peerDependencies": {}
}
