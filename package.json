{
   "name": "prensa-prensa",
   "version": "1.0.0",
   "engines": {
      "node": "16.13.0",
      "pnpm": "6.24.2"
   },
   "scripts": {
      "prepare": "husky install",
      "preinstall": "npx only-allow pnpm",
      "frontend-www:dev": "pnpm dev --filter frontend-www",
      "frontend-www:build": "pnpm build --filter frontend-www",
      "frontend-www:start": "pnpm start --filter frontend-www",
      "frontend-www:lint": "pnpm lint --filter frontend-www",
      "frontend-www:test": "pnpm test --filter frontend-www",
      "frontend-www:test:watch": "pnpm test:watch --filter frontend-www",
      "frontend-www:storybook": "pnpm storybook --filter frontend-www",
      "frontend-www:storybook:build": "pnpm storybook:build --filter frontend-www",
      "frontend-www:format": "pnpm format --filter frontend-www",
      "frontend-www-e2e:lint": "pnpm lint --filter frontend-www-e2e",
      "frontend-www-e2e:e2e": "pnpm e2e --filter frontend-www-e2e",
      "frontend-www-e2e:report": "pnpm report --filter frontend-www-e2e",
      "frontend-www-e2e:format": "pnpm format --filter frontend-www-e2e",
      "backend-gateway:dev": "pnpm dev --filter backend-gateway",
      "backend-gateway:build": "pnpm build --filter backend-gateway",
      "backend-gateway:start": "pnpm start --filter backend-gateway",
      "backend-gateway:test": "pnpm test --filter backend-gateway",
      "backend-gateway-e2e:lint": "pnpm lint --filter backend-gateway-e2e",
      "backend-gateway-e2e:e2e": "pnpm e2e --filter backend-gateway-e2e",
      "backend-gateway-e2e:report": "pnpm report --filter backend-gateway-e2e",
      "backend-gateway-e2e:format": "pnpm format --filter backend-gateway-e2e",
      "backend-services-articles-command:dev": "pnpm dev --filter backend-services-articles-command",
      "backend-services-articles-command:build": "pnpm build --filter backend-services-articles-command",
      "backend-services-articles-command:start": "pnpm start --filter backend-services-articles-command",
      "backend-services-articles-command:lint": "pnpm lint --filter backend-services-articles-command",
      "backend-services-articles-command:format": "pnpm format --filter backend-services-articles-command",
      "backend-services-articles-command-e2e:lint": "pnpm lint --filter backend-services-articles-command-e2e",
      "backend-services-articles-command-e2e:e2e": "pnpm e2e --filter backend-services-articles-command-e2e",
      "backend-services-articles-command-e2e:report": "pnpm report --filter backend-services-articles-command-e2e",
      "backend-services-articles-command-e2e:format": "pnpm format --filter backend-services-articles-command-e2e",
      "backend-services-articles-query:dev": "pnpm dev --filter backend-services-articles-query",
      "backend-services-articles-query:build": "pnpm build --filter backend-services-articles-query",
      "backend-services-articles-query:start": "pnpm start --filter backend-services-articles-query",
      "backend-services-articles-query:lint": "pnpm lint --filter backend-services-articles-query",
      "backend-services-articles-query:format": "pnpm format --filter backend-services-articles-query",
      "backend-services-articles-query-e2e:lint": "pnpm lint --filter backend-services-articles-query-e2e",
      "backend-services-articles-query-e2e:e2e": "pnpm e2e --filter backend-services-articles-query-e2e",
      "backend-services-articles-query-e2e:report": "pnpm report --filter backend-services-articles-query-e2e",
      "backend-services-articles-query-e2e:format": "pnpm format --filter backend-services-articles-query-e2e",
      "all:start": "pnpm start -r --filter ./",
      "all:build": "pnpm build -r --filter ./",
      "all:lint": "pnpm lint -r --filter ./",
      "all:test": "pnpm test --filter ./",
      "all:e2e": "pnpm e2e --filter ./",
      "all:format": "pnpm format -r --filter ./",
      "clean": "rimraf **/node_modules/** **/dist/** **/.next/** **/coverage/**"
   },
   "devDependencies": {
      "husky": "7.0.4",
      "rimraf": "3.0.2"
   }
}
