{
   "name": "prensa-prensa",
   "version": "1.0.0",
   "engines": {
      "node": "16.13.0"
   },
   "scripts": {
      "prepare": "husky install",
      "preinstall": "npx only-allow pnpm",
      "frontend-www:dev": "pnpm dev --filter frontend-www",
      "frontend-www:build": "pnpm build --filter frontend-www",
      "frontend-www:start": "pnpm start --filter frontend-www",
      "frontend-www:lint": "pnpm lint --filter frontend-www",
      "frontend-www:test": "pnpm test --filter frontend-www",
      "frontend-www:test:watch": "pnpm test:watch --filter frontend-www",
      "frontend-www:storybook": "pnpm storybook --filter frontend-www",
      "frontend-www:storybook:build": "pnpm storybook:build --filter frontend-www",
      "frontend-www:format": "pnpm format --filter frontend-www",
      "frontend-www-e2e:lint": "pnpm lint --filter frontend-www-e2e",
      "frontend-www-e2e:start": "pnpm start --filter frontend-www-e2e",
      "frontend-www-e2e:report": "pnpm report --filter frontend-www-e2e",
      "frontend-www-e2e:format": "pnpm format --filter frontend-www-e2e"
   },
   "devDependencies": {
      "husky": "7.0.4"
   }
}
